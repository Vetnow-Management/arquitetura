{
  "name": "@vetnow-management/arquitetura",
  "private": true,
  "version": "0.0.1",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "bootstrap": "concurrently \"lerna bootstrap --use-workspaces\" \"lerna exec yarn prep-local\" ",
    "build": "lerna exec -- yarn build",
    "test": "lerna run test",
    "clean": "lerna exec -- yarn clean-cache",
    "storybook": "lerna exec --scope @vetnow-management/components yarn start",
    "exec-publish": "lerna exec -- yarn pack",
    "remove-node_modules": "concurrently \"lerna exec yarn remove-node_modules\" \"rm -rf ./node_modules\" "
  },
  "license": "ISC",
  "devDependencies": {
    "lerna": "^3.22.1",
    "typescript": "^4.0.3",
    "concurrently": "^5.3.0",
    "rimraf": "^3.0.2"
  },
  "engines": {
    "node": ">=0.12"
  },
  "dependencies": {
  }
}
